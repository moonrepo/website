{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "verbatimModuleSyntax": true,
    "paths": {
      "#components/*": [
        "./src/components/*"
      ],
      "#layouts/*": [
        "./src/layouts/*"
      ]
    }
  }
}
